<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.quick.tracking">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.quick.tracking.Main5Activity"></activity>
        <activity android:name="com.quick.tracking.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.quick.tracking.MainActivity" />
        <activity android:name="com.quick.tracking.Main4Activity" />
        <activity android:name="com.quick.tracking.Main3Activity" />
        <activity android:name="com.quick.tracking.Main2Activity" />
    </application>

</manifest>